[SERVICE]
# Operations logging
LOG_ACTIVE = True

[WORLDS]
# Ontology filename (.owl)
FILE_NAME = test.owl
# number of worlds
NUMBER_OF_WORLDS = 1


# World #1 initialization
[INIT-W1]
WORLD_NAME = World1
WORLD_ind = World1_ind

# Beliefs
BELIEFS = Belief1, Belief2, Belief3
BELIEFS_ind = Belief1_ind, Belief2_ind, Belief3_ind

# Desires
DESIRES = Desire1, Desire2, Desire3
DESIRES_ind = Desire1_ind, Desire2_ind, Desire3_ind

# Intentions
INTENTIONS = Intention1, Intention2, Intention3
INTENTIONS_ind = Intention1_ind, Intention2_ind, Intention3_ind

# Plans
PLANS = Plan1, Plan2, Plan3
PLANS_ind = Plan1_ind, Plan2_ind, Plan3_ind

# Agents
AGENTS = Agent-A, Agent-B, Agent-C

# Agent-A individuals
Agent-A_ind = Agent-A_ind
# Agent-A beliefs
Agent-A_ind_beliefs = Belief1_ind, Belief2_ind, Belief3_ind
# Agent-A desires
Agent-A_ind_desires = Desire1_ind
# Agent-A intentions
Agent-A_ind_intentions = Intention1_ind
# Agent-A plans
Agent-A_ind_plans = Plan1_ind


# Agent-B individuals
Agent-B_ind = Agent-B_ind
# Agent-B beliefs
Agent-B_ind_beliefs = Belief1_ind, Belief2_ind, Belief3_ind
# Agent-B desires
Agent-B_ind_desires = Desire2_ind
# Agent-B intentions
Agent-B_ind_intentions = Intention2_ind
# Agent-B plans
Agent-B_ind_plans = Plan2_ind

# Agent-C individuals
Agent-C_ind = Agent-C_ind
# Agent-C beliefs
Agent-C_ind_beliefs = Belief1_ind, Belief2_ind, Belief3_ind
# Agent-C desires
Agent-C_ind_desires = Desire3_ind
# Agent-C intentions
Agent-C_ind_intentions = Intention3_ind
# Agent-C plans
Agent-C_ind_plans = Plan3_ind


[SWRL]
NUMBER_OF_RULES = 3
RULE1 = Agent-A(?x1), hasWorld(?x1,?y1), World1(?y1) -> hasAgent(?y,?x)
RULE2 = Agent-B(?x2), hasWorld(?x2,?y2), World1(?y2) -> hasAgent(?y2,?x2)
RULE3 = Agent-C(?x3), hasWorld(?x3,?y3), World1(?y3) -> hasAgent(?y3,?x3)


[REASONING]
# HERMIT, PELLET
REASONER = HERMIT
# Save also inferred triples in FILE_NAME
SAVE_AFTER_INFER = false

